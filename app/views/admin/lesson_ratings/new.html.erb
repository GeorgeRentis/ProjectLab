<%= form_for([:admin,@lesson_rating]) do |f|  %>
  <div class = "form-group">
    <%= f.label :communication,class: "control-label"%>
    <%= f.number_field :communication,class: "form-control" %>
  </div>
  <div class = "form-group">
    <%= f.label :quality,class: "control-label"%>
    <%= f.number_field :quality,class: "form-control" %>
  </div>
  <div class = "form-group">
    <%= f.label :teaching,class: "control-label"%>
    <%= f.number_field :teaching,class: "form-control" %>
  </div>
    <div class = "form-group">
    <%= f.label :general_grade,class: "control-label"%>
    <%= f.number_field :general_grade,class: "form-control" %>
  </div>
  <%= f.hidden_field :user_id, :value => current_user.id %>
  <%= f.hidden_field :lesson_id, :value =>params[:lesson_id] %>
  <%= f.submit class: "btn btn-success" %>
<% end %>